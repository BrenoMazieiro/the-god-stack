{
    "env": {
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "semi": [2, "never"],
      "import/extensions": [2, "always"],
      "import/prefer-default-export": "off"
    }
}
